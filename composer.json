{
    "require-dev": {
        "pestphp/pest": "^2.13",
        "symfony/var-dumper": "^6.3",
        "mockery/mockery": "^1.6",
        "pestphp/pest-plugin-type-coverage": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "ext-pdo": "*",
        "nikic/fast-route": "^1.3",
        "league/container": "^4.2",
        "symfony/dotenv": "^6.3",
        "firebase/php-jwt": "^6.9"
    }
}
